from traceback import print_stack


temperature = 15 
if temperature > 30:
     print("It's warm")
     print("Drink water") 
elif temperature > 20:
     print("it's nice")
else:
    print("it's cold")
print("Done")

#ternary operator

age = 22
message = "Eligible" if age >= 18 else "Not eligible"
print(message)

#operators
#and or not

high_income = False
good_credit = True
student=False
if high_income  and good_credit:
     print("Eligible")
else:
     print("Not eligible") 

if not student:
     print("Eligible")
else:
          print("NOt eligible")


# simple implementation

if (high_income or good_credit) and not student:
     print("Eligible")
else:
     print("Not Eligible")

